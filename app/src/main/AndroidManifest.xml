<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaora.anunciosapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.TermosDeUsoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.CategoriasActivity"
            android:label="Categorias" />
        <activity
            android:name=".activities.PreferenciasActivity"
            android:label="Interesses" />
        <activity
            android:name=".activities.AnunciantesActivity"
            android:label="Anunciantes" />
        <activity android:name=".activities.AnuncianteActivity" />
        <activity android:name=".activities.NovoAnuncioActivity" />
        <activity android:name=".activities.MeusAnunciosActivity" />
    </application>

</manifest>
